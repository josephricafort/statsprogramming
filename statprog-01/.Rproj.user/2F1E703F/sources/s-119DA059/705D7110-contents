month.abb

# indexing using positive integers
month.abb[c(1, 2, 3)]
month.abb[c(1, 1, 1, 1, 1, 2, 3)]

# selecting using negative integers
month.abb[c(-1, -2)]
month.abb[-(1:10)]

# selecting using a logical vector
month.abb[c(T, T, T, T, F, F, F, F, F, F, F)]
month.abb[c(T, F)]
month.abb[c(T, F, T, F, F, T, F, T, F, T, F)]

# selecting using names
x <- 1:12
names(x) <- month.abb
x
x[c('Jan', 'Apr', 'Jan', 'Dec')]

my_list[1]
my_list[[1]]

my_data